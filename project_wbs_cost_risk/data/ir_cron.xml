<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">
    <record id="project_budget_hours_cost_risk" model="ir.cron">
        <field name="name">Project Budget Hours Cost Risk</field>
        <field eval="True" name="active" />
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field
            name="nextcall"
            eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')"
        />
        <field name="model_id" ref="analytic.model_account_analytic_account" />
         <field name="code">model.cron_calculate_cost_risk()</field>
        <field eval="False" name="doall" />
        <field name="priority">5</field>
    </record>
</odoo>
