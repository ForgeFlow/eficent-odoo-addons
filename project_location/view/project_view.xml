<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="edit_project_stock_move" model="ir.ui.view">
        <field name="name">project.project.form.stock.move</field>
        <field name="model">project.project</field>
        <field name="inherit_id" ref="project.edit_project" />
        <field name="arch" type="xml">
            <xpath expr="///group[@name='extra_settings']" position="before">
                <group name="stock">
                    <field name="project_analytic_id" invisible="1" />
                    <field
                        name="location_id"
                        context="{'default_analytic_account_id': project_analytic_id}"
                    />
                    <field name="picking_type_id" />
                    <field name="dest_address_id" />
                </group>
            </xpath>
        </field>
    </record>

</odoo>
