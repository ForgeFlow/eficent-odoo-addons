<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="view_account_analytic_account_wip_tree" model="ir.ui.view">
            <field name="name">account.analytic.account.wip.tree</field>
            <field name="model">account.analytic.account</field>
            <field name="inherit_id"
                   ref="analytic_wip_report.view_account_analytic_account_wip_tree"/>
            <field name="arch" type="xml">
                <field name="under_billings" position="after">
                    <field name="fy_revenue"/>
                    <field name="fy_costs"/>
                    <field name="fy_gross_profit"/>
                    <field name="fy_actual_costs"/>
                    <field name="fy_actual_material_cost"/>
                    <field name="fy_actual_labor_cost"/>
                </field>
            </field>
        </record>

    </data>
</openerp>
