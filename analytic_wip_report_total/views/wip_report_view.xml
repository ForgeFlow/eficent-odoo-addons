<odoo>

    <record id="view_account_analytic_account_wip_tree_total1" model="ir.ui.view">
        <field name="name">account.analytic.account.wip.tree</field>
        <field name="model">account.analytic.account</field>
        <field name="inherit_id" ref="analytic_wip_report.view_account_analytic_account_wip_tree"/>
        <field name="arch" type="xml">
            <field name="total_value" position="attributes">
                <attribute name="sum">Total</attribute>
            </field>
            <field name="actual_billings" position="attributes">
                <attribute name="sum">Total</attribute>
            </field>
            <field name="actual_costs" position="attributes">
                <attribute name="sum">Total</attribute>
            </field>
            <field name="total_estimated_costs" position="attributes">
                <attribute name="sum">Total</attribute>
            </field>
            <field name="estimated_costs_to_complete" position="attributes">
                <attribute name="sum">Total</attribute>
            </field>
            <field name="estimated_gross_profit" position="attributes">
                <attribute name="sum">Total</attribute>
            </field>
            <field name="earned_revenue" position="attributes">
                <attribute name="sum">Total</attribute>
            </field>
            <field name="over_billings" position="attributes">
                <attribute name="sum">Total</attribute>
            </field>
            <field name="under_billings" position="attributes">
                <attribute name="sum">Total</attribute>
            </field>
        </field>
    </record>

    <record id="view_account_analytic_account_wip_tree_total3" model="ir.ui.view">
        <field name="name">account.analytic.account.wip.tree</field>
        <field name="model">account.analytic.account</field>
        <field name="inherit_id" ref="analytic_wip_report_extend.view_account_analytic_account_wip_extend_tree"/>
        <field name="arch" type="xml">
            <field name="under_over" position="attributes">
                <attribute name="sum">Total</attribute>
            </field>
        </field>
    </record>

</odoo>
