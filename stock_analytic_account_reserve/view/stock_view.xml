<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
        <record id="view_move_form" model="ir.ui.view">
            <field name="name">stock.move.form</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock_analytic_account.view_move_form"/>
            <field eval="1" name="priority"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='analytic_account_id']" position="after">
                    <field name="analytic_reserved"/>
                </xpath>
            </field>
        </record>

        <record id="view_move_tree" model="ir.ui.view">
            <field name="name">stock.move.tree</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock_analytic_account.view_move_tree"/>
            <field eval="1" name="priority"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='analytic_account_id']" position="after">
                	<field name="analytic_account_id" />
                </xpath>
            </field>
        </record>


        <record id="view_move_picking_tree" model="ir.ui.view">
            <field name="name">stock.move.tree</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock_analytic_account.view_move_picking_tree"/>
            <field eval="4" name="priority"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='analytic_account_id']" position="after">
                    <field name="analytic_reserved"/>
                </xpath>
            </field>
        </record>


        <record id="view_move_search_analytic" model="ir.ui.view">
            <field name="name">stock.move.search</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock_analytic_account.view_move_search"/>
            <field eval="3" name="priority"/>
            <field name="arch" type="xml">        
                <xpath expr="//field[@name='name']" position="after">
                    <field name="analytic_reserved"/>
                </xpath>
            </field>
        </record>   

        <record id="view_move_tree_reception_picking_analytic" model="ir.ui.view">
            <field name="name">stock.move.tree2</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock_analytic_account.view_move_tree_reception_picking"/>
            <field name="priority" eval="6"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='analytic_account_id']" position="after">
                    <field name="analytic_reserved"/>
                </xpath>
            </field>
        </record>

        #
        # Lot composition (history)
        #
        <record id="stock_move_tree" model="ir.ui.view">
            <field name="name">Stock Moves</field>
            <field name="model">stock.move</field>
            <field name="field_parent">move_history_ids</field>
            <field name="inherit_id" ref="stock_analytic_account.stock_move_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='analytic_account_id']" position="after">
                    <field name="analytic_reserved"/>
                </xpath>
            </field>
        </record>

    </data>
</openerp>
